[application]

config/name="papercut"
config/description="[Add your description here!]"
run/main_scene="res://scenes/game_flow/gameplay/game.tscn"
config/features=PackedStringArray("4.1", "Mobile")
run/max_fps=1000
config/icon="res://assets/icon.svg"

[autoload]

Global="*res://globals/global.gd"
Sound="*res://globals/sound.gd"
